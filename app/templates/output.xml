<rss version="2.0">
    <channel>
        {% for job in jobs %}
        <item>
            <title>{{ job[2] }}</title>
            <description>{{ job[1] }}</description>
            <pubDate>{{ job[3] }}</pubDate>
            <guid>{{ job[0] }}</guid>
            <link>{{ job[4] }}</link>
        </item>
        {% endfor %}
    </channel>
</rss>
